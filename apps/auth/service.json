{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "auth-service"
  },
  "spec": {
    "type": "NodePort",
    "selector": {
      "app": "auth"
    },
    "ports": [
      {
        "name": "auth-service",
        "port": 80,
        "targetPort": 8000,
        "protocol": "TCP"
      }
    ]
  }
}
